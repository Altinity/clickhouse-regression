<yandex>
  <storage_configuration>
      <disks>
          <default>
              <keep_free_space_bytes>1024</keep_free_space_bytes>
            </default>
            <jbod1>
              <path>/jbod1/</path>
            </jbod1>
            <jbod2>
              <path>/jbod2/</path>
                <keep_free_space_bytes>10485760</keep_free_space_bytes>
            </jbod2>
            <jbod3>
              <path>/jbod3/</path>
                <keep_free_space_ratio>0.5</keep_free_space_ratio>
            </jbod3>
            <external>
              <type>s3</type>
                <endpoint>http://minio:9001/root/data/</endpoint>
                <access_key_id>minio</access_key_id>
                <secret_access_key>minio123</secret_access_key>
            </external>
        </disks>
        <policies>
          <one_small_disk>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                </volumes>
            </one_small_disk>
            <small_jbod_with_external>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
            </small_jbod_with_external>
            <jbods>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                        <disk>jbod2</disk>
                    </main>
                </volumes>
            </jbods>
            <jbods_with_external>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                        <disk>jbod2</disk>
                        <max_data_part_size_bytes>10485760</max_data_part_size_bytes>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
            </jbods_with_external>
            <jbods_with_external_ratio>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                        <disk>jbod2</disk>
                        <max_data_part_size_ratio>0.25</max_data_part_size_ratio>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
            </jbods_with_external_ratio>
            <moving_jbod_with_external>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
                <move_factor>0.7</move_factor>
            </moving_jbod_with_external>
            <moving_max_jbod_with_external>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
                <move_factor>1</move_factor>
            </moving_max_jbod_with_external>
            <moving_min_jbod_with_external>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                    <external>
                      <disk>external</disk>
                    </external>
                </volumes>
                <move_factor>0</move_factor>
            </moving_min_jbod_with_external>
            <default_disk_with_external>
              <volumes>
                  <small>
                      <disk>default</disk>
                        <max_data_part_size_bytes>2097152</max_data_part_size_bytes>
                    </small>
                    <big>
                      <disk>external</disk>
                        <max_data_part_size_bytes>20971520</max_data_part_size_bytes>
                    </big>
                </volumes>
            </default_disk_with_external>
            <jbod1_with_jbod2>
              <volumes>
                  <main>
                      <disk>jbod1</disk>
                    </main>
                    <external>
                      <disk>jbod2</disk>
                    </external>
                </volumes>
            </jbod1_with_jbod2>
            <fast_med_and_slow>
              <volumes>
                  <fast>
                      <disk>jbod1</disk>
                    </fast>
                    <medium>
                      <disk>jbod2</disk>
                    </medium>
                    <slow>
                      <disk>external</disk>
                    </slow>
                </volumes>
            </fast_med_and_slow>
            <only_jbod2>
              <volumes>
                  <main>
                      <disk>jbod2</disk>
                    </main>
                </volumes>
            </only_jbod2>
            <only_jbod3>
              <volumes>
                  <main>
                      <disk>jbod3</disk>
                    </main>
                </volumes>
            </only_jbod3>
            <special_warning_policy>
              <volumes>
                  <special_warning_zero_volume>
                      <disk>default</disk>
                        <max_data_part_size_bytes>0</max_data_part_size_bytes>
                    </special_warning_zero_volume>
                    <special_warning_default_volume>
                      <disk>external</disk>
                    </special_warning_default_volume>
                    <special_warning_small_volume>
                      <disk>jbod1</disk>
                        <max_data_part_size_bytes>1024</max_data_part_size_bytes>
                    </special_warning_small_volume>
                    <special_warning_big_volume>
                      <disk>jbod2</disk>
                        <max_data_part_size_bytes>1024000000</max_data_part_size_bytes>
                    </special_warning_big_volume>
                </volumes>
            </special_warning_policy>
        </policies>
    </storage_configuration>
</yandex>

